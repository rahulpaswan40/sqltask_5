-- INNER JOIN
SELECT TourPackages.Destination, TourPackages.Duration, TourPackages.Price,
       TravelAgencies.AgencyName, TravelAgencies.Country
FROM TourPackages
INNER JOIN TravelAgencies ON TourPackages.AgencyID = TravelAgencies.AgencyID;

-- LEFT JOIN
SELECT TourPackages.Destination, TourPackages.Duration, TravelAgencies.AgencyName
FROM TourPackages
LEFT JOIN TravelAgencies ON TourPackages.AgencyID = TravelAgencies.AgencyID;

-- RIGHT JOIN
SELECT TravelAgencies.AgencyName, TravelAgencies.Country, TourPackages.Destination
FROM TourPackages
RIGHT JOIN TravelAgencies ON TourPackages.AgencyID = TravelAgencies.AgencyID;
